<section class="box-header">
  <div class="row">
    <div class="col-xs-6">
      <h1><%= @marketplace.name %>'s admin users</h1>
    </div>
    <div class="col-xs-6 text-right">
      <a href="<%= new_admin_marketplace_admin_user_path(@marketplace) %>" class="btn sitebtn">Add Admin User</a>
    </div>
  </div>
</section>

<section class="breadcrumb">
  <ul>
    <li>
      <a href="<%= admin_marketplaces_path %>">Marketplaces</a>
    </li>
    <li>
      <a href="<%= admin_marketplace_path(@marketplace) %>"><%= @marketplace.name %></a>
    </li>
    <li>
      <a href="<%= admin_marketplace_admin_users_path(@marketplace) %>">Admin users</a>
    </li>
    <li class="active">Add new</li>
  </ul>
</section>

<section class="content">
  <div class="box">
    <div class="row">
      <div class="col-sm-6 col-xs-12">
        <%= render 'form', marketplace: @marketplace, admin_user: @admin_user %>
      </div>
    </div>
  </div>
</section>
